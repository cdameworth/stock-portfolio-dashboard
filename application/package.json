{
  "name": "stock-portfolio-dashboard",
  "version": "1.0.0",
  "description": "Web dashboard for stock portfolio management and recommendations",
  "main": "server.js",
  "scripts": {
    "start": "node -r ./tracing.js server.js",
    "dev": "nodemon -r ./tracing.js server.js",
    "dev:react": "./scripts/react-dev.sh",
    "build:react": "vite build --config vite.config.react.js",
    "preview:react": "vite preview --config vite.config.react.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:services": "jest tests/services/",
    "test:integration": "jest tests/integration/",
    "test:e2e": "jest tests/e2e/",
    "test:ci": "jest --coverage --watchAll=false --ci",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "npm run build:css && npm run build:js && npm run build:react",
    "build:css": "postcss public/css/main.css -o public/dist/main.min.css",
    "build:js": "webpack --mode production",
    "migrate": "node scripts/run-migrations.js",
    "migrate:dev": "DB_HOST=localhost DB_NAME=stock_portfolio DB_USER=postgres DB_PASSWORD=postgres node scripts/run-migrations.js",
    "health": "node -e \"require('./services/base-service'); console.log('Services loaded successfully')\"",
    "test:otel": "node test-otel.js",
    "validate": "npm run lint && npm run format:check && npm run test:ci"
  },
  "keywords": [
    "stock",
    "portfolio",
    "dashboard",
    "finance"
  ],
  "author": "Frontend Team",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@mui/x-data-grid": "^8.11.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.64.1",
    "@opentelemetry/exporter-otlp-http": "^0.26.0",
    "@opentelemetry/propagator-aws-xray": "^2.1.1",
    "@opentelemetry/resource-detector-aws": "^2.5.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.6.1",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "branca": "^0.5.0",
    "chart.js": "^4.5.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "elastic-apm-node": "^4.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-winston": "^4.2.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "newrelic": "^10.5.0",
    "node-cache": "^5.1.2",
    "pg": "^8.11.3",
    "postcss-cli": "^11.0.1",
    "prom-client": "^15.1.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "redis": "^4.6.0",
    "stripe": "^20.1.2",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "yahoo-finance2": "^3.0.0",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.6.2",
    "cssnano": "^7.1.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "vite": "^7.1.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
